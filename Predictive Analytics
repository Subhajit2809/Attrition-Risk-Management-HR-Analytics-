setwd("D:/r")
default <- read.csv("HR_comma_sep.csv")
str(default)
summary(default)
names <- c(6,7,8:10)
default[,names] <- lapply(default[,names] , factor)
str(default)
model.glm <- glm(left ~ ., family=binomial, data=default)
summary(model.glm)
default$pred <- predict(model.glm, newdata = default, type = "response")
head(default)
last_subset <- subset(default, left==0&pred>0.80&last_evaluation>0.80)
plot(default$pred,default$last_evaluation, type = "n")
